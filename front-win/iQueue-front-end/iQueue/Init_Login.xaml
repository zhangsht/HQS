<Window x:Class="iQueue.Init_Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iQueue"
        mc:Ignorable="d"
        Title="Init_Login" MaxHeight="350" MaxWidth="500" MinHeight="350" MinWidth="500" Height="350" Width="500">
    <Grid>
        <TextBlock Text="账号" Width="60" Height="30" Margin="0,120,70,0"></TextBlock>
        <TextBox Height="30" Margin="86,100,0,0" Width="120" Name="username"/>
        <Button Name="Send" Click="Send_Click" Width="59" Height="30" Margin="0,260,0,0" >登陆</Button>
        <TextBlock Text="密码" Width="59" Height="30" Margin="0,190,70,0"/>
        <TextBox Height="30" Margin="86,179,0,0" Width="120" Name="password"/>
        <Image Name="IconV1" Source=".\icon\IconV1.jpg" Margin="0,0,0,200" Width="100"/>
        <TextBlock Text="门诊分诊挂号系统" Width="100" Height="30" />
    </Grid>
</Window>
